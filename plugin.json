{
  "id": "nodebb-plugin-flowprompt-sso",
  "name": "FlowPrompt SSO",
  "description": "JWT-based SSO integration with FlowPrompt",
  "url": "https://github.com/flowprompt/nodebb-plugin-flowprompt-sso",
  "library": "./library.js",
  "hooks": [
    {
      "hook": "static:app.load",
      "method": "init"
    },
    {
      "hook": "filter:scripts.get",
      "method": "filterScriptsGet"
    }
  ],
  "templates": "./templates",
  "languages": "./languages"
}
